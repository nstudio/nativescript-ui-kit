<Page xmlns="http://schemas.nativescript.org/tns.xsd" navigatingTo="navigatingTo" class="page">
  <Page.actionBar>
    <ActionBar title="Coach Marks for NativeScript">
      <!-- <ActionBar.actionItems>
              <ActionItem id="menu" text="Menu" />
          </ActionBar.actionItems> -->
      <ActionBar.titleView>
        <GridLayout paddingLeft="5" paddingRight="5" loaded="{{loadedMenuItem}}">
          <Label id="menu" text="Menu"/>
        </GridLayout>
      </ActionBar.titleView>
    </ActionBar>
  </Page.actionBar>
  <StackLayout>
    <Button text="Customer Listing" class="message msg" id="first" />
    <Label text="1" class="message msg2" textWrap="true" id="second" />
    <Button text="Account Settings" class="message msg msg3" id="third" />

    <Button text="Start with Custom Options" tap="{{startWithOptions}}" class="message msg msg4" id="fourth" />

  </StackLayout>
</Page>