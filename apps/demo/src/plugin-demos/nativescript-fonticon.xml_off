<Page xmlns="http://schemas.nativescript.org/tns.xsd" navigatingTo="navigatingTo" class="page">
    <Page.actionBar>
        <ActionBar title="nativescript-fonticon" icon="" class="action-bar">
        </ActionBar>
    </Page.actionBar>
    <StackLayout class="p-20">
        <ScrollView>
            <StackLayout>
              <Label class="fa" text="{{'fa-glass' | fonticon}}"></Label>
              <Label class="fa" text="{{'fa-music' | fonticon}}"></Label>
              <Label class="fa" text="{{'fa-search' | fonticon}}"></Label>
              <Label class="fa" text="{{'fa-heart' | fonticon}}"></Label>
              <Label class="fa" text="{{'fa-star' | fonticon}}"></Label>
              <Label class="fa" text="{{'fa-user' | fonticon}}"></Label>
