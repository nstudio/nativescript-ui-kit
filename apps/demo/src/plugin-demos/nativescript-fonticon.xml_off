              <Label class="fa" text="{{'fa-check' | fonticon}}"></Label>
              <Label class="fa" text="{{'fa-power-off' | fonticon}}"></Label>
              <Label class="fa" text="{{'fa-signal' | fonticon}}"></Label>
              <Label class="fa" text="{{'fa-gear' | fonticon}}"></Label>
              <Label class="fa" text="{{'fa-bluetooth' | fonticon}}"></Label>
              <Label class="fa" text="{{'fa-refresh' | fonticon}}"></Label>
              <Label class="fa" text="{{'fa-lock' | fonticon}}"></Label>
              <Label class="fa" text="{{'fa-flag' | fonticon}}"></Label>
